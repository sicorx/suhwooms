label { display: block; text-align: left; vertical-align: middle; box-sizing: border-box; margin: 0 auto; margin-bottom: 12px; text-align: center; }
	label.no_style { /*background: none;*/ display: inline-block; margin: 0; padding: 0; }
	label.check { margin-left: 12px; padding-left: 18px; background: url(../../images/mbl/icon_login_chk_off.png) no-repeat left center; background-size: auto 12px; cursor: pointer; vertical-align: middle;  }
		label.check.checked { background-image: url(../../images/mbl/icon_login_chk_on.png); }

	label.radio { margin-left: 12px; padding-left: 18px; background: url(../../images/mbl/icon_radio_off.png) no-repeat left center; background-size: auto 12px; cursor: pointer; vertical-align: middle; }
		label.radio.checked { background-image: url(../../images/mbl/icon_radio_on.png); }

			label.check.checked span,
			label.radio.checked span { color: #049571 !important; }
		label.check input,
		label.radio input { vertical-align: middle; margin-left: 12px; position: absolute; border: none; z-index: 0; width: 0; height: 0; visibility: hidden; display: none; }
