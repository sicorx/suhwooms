.state_slide { display: block; position: relative; box-sizing: border-box; /*border-bottom: 2px solid rgba(0,0,0,0.2);*/ background-color: rgba(255,255,255,0.5); box-shadow: 0 1px rgba(0,0,0,0.2); }
	.state_slide .slide_group { position: relative; width: 248px; height: 48px; margin: 0 auto; overflow: hidden; }
		.state_slide .slide_group ul { display: block; position: absolute; width: 100%; height: 100%; top: 0; left: 0; *zoom: 1; }
			.state_slide .slide_group ul:after { content: ''; clear: both; display: block; }
			.state_slide .slide_group ul li { position: relative; float: left; text-align: center; color: #50597b; font-size: 14px; font-weight: bold; vertical-align: }
	.state_slide .slide_control { z-index: 99; position: absolute; top: 0; width: 100%; height: 0; overflow: visible; }
		.state_slide .slide_control a { position: absolute; width: 40px; height: 48px; top: 0; }
		.state_slide .slide_control .slide_prev { left: 0; background: url(../../images/mbl/arrow_slide_prev.png) no-repeat right center; background-size: 25px auto; }
		.state_slide .slide_control .slide_next { right: 0; background: url(../../images/mbl/arrow_slide_next.png) no-repeat left center; background-size: 25px auto; }

.state_content { position: relative; }
	.state_content h1 { position: relative; text-align: center; height: 47px; line-height: 47px; box-sizing: border-box; /*border-bottom: 2px solid rgba(0,0,0,0.2);*/ color: #50597b; font-size: 14px; font-weight: bold; box-shadow: 0 1px rgba(0,0,0,0.2); }
	.state_content .switch_content { position: relative; z-index: 99; width: 100%; height: 0; top: 0; overflow: visible; }
		.state_content .switch_content a { position: absolute; width: 80px; height: 80px; top: 15px; background: no-repeat center center; background-size: 80px 80px; }
			.state_content .switch_content a.month { left: 15px; background-image: url(../../images/mbl/btn_state_month_off.png); }
				.state_content .switch_content a.month.on { left: 15px; background-image: url(../../images/mbl/btn_state_month_on.png); }
			.state_content .switch_content a.day { right: 15px; background-image: url(../../images/mbl/btn_state_day_off.png); }
				.state_content .switch_content a.day.on { right: 15px; background-image: url(../../images/mbl/btn_state_day_on.png); }

	.state_content .count_value { position: relative; height: 285px; padding-top: 93px; box-sizing: border-box; margin-bottom: 25px; }
		.state_content .count_value > div { position: absolute; transform: rotate(45deg); -ms-transform: rotate(45deg); -webkit-transform: rotate(45deg); transform-origin: 0 0; -ms-transform-origin: 0 0; -webkit-transform-origin: 0 0; }

		.state_content .count_value .before { position: absolute; background-color: #a0d468; border-radius: 3px; box-sizing: border-box; left: 50%; margin-left: -66px; color: #fff; width: 90px; height: 90px; }
		.state_content .count_value .now { position: absolute; background-color: none; border-radius: 3px; box-sizing: border-box; left: 50%; border: 2px solid #40c269; margin-left: 66px; color: #40c269; width: 90px; height: 90px; }
		.state_content .count_value .forecast { position: absolute; background-color: #4fc1e9; border-radius: 3px; box-sizing: border-box; top: 158px; left: 50%; color: #fff; width: 90px; height: 90px; }

		.state_content .count_value .update_time { transform: rotate(0deg); -ms-transform: rotate(0deg); -webkit-transform: rotate(0deg); font-size: 11px; color: #333; left: 50%; margin-left: 20px; letter-spacing: -0.05em; top: 270px; }

		.state_content .count_value > div p { position: relative; width: 100%; height: 100%; transform: rotate(-45deg); -ms-transform: rotate(-45deg); -webkit-transform: rotate(-45deg); transform-origin: 50% 50%; -ms-transform-origin: 50% 50%; -webkit-transform-origin: 50% 50%; text-align: center; box-sizing: border-box; padding-top: 23px; }
			.state_content .count_value > div p .number { display: inline-block; font-size: 26.667px; font-weight: bold; }
			.state_content .count_value > div p .unit { display: inline-block; font-size: 13.333px; font-weight: bold; }
			.state_content .count_value > div p .title { display: block; font-size: 13.333px; font-weight: bold; text-align: center; }

	.state_content .btn_group { *zoom: 1; text-align: left; }
