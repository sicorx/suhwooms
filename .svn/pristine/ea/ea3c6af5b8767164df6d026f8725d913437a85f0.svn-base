/* Grid */
/* .r_grid {background:#fcfcfd; border:1px solid #cbcbcb; border-radius:3px 3px 0 0; position:relative;} */
.r_grid {background:#f3f4f6; border:0; border-radius:0; position:relative;} /* 2017.02.01 수정 */
/* .r_gridTitle {background:#ffffff url(/images/gr_001.gif) repeat-x scroll left bottom; border-bottom:1px solid #cbcbcb; position:relative; height:31px; line-height:31px;} */
.r_gridTitle {position:relative; margin:15px 0 6px 0; height:22px;} /* 2017.02.06 수정 */
.r_gridTitle.top_table {position:relative; margin:0 0 6px 0; height:22px;} /* 2017.02.07 추가 */
/* .r_gridTitle h3 {font-size:14px; margin:0 0 0 15px; float:left; font-weight:bold; color:#333333;} */
.r_gridTitle h3 {font-family:"Malgun Gothic","NanumGothic", Dotum !important; font-size:13px; margin:0px; padding-left:10px; float:left; font-weight:normal; color:#222222; background:url(/images/icon_page_title.png) 0 7px no-repeat;} /* 2017.02.06 수정 */
.r_gridTitle img {cursor:pointer;}

.r_grid table tr {background-color:#ffffff;}
/* .r_grid table tr.r_even {background-color:#f8f9fa;} */ 
.r_grid table tr:hover {background-color:#fff7fa;}
.r_grid table tr.r_select {background-color:#ffeff5;}
.r_grid table th {font-weight:bold; color:#666666; border-bottom:1px solid #cbcbcb; padding:10px 0; border-right:1px solid #e8e8e8; background-color:#e5e5e5 !important;}
.r_grid table th.r_last {border-right:none;}
.r_grid table tr.r_last td {border-bottom:none;}
.r_grid table td {color:#222222;border-bottom:1px solid #e8e8e8; padding:7px 8px; text-align:center; vertical-align:middle; line-height:19px;}
.r_grid table tr.r_emptyBtm td {border-bottom:1px solid #cbcbcb;}
.r_grid table td.r_last {border-right:none;}
.r_grid table td.r_gridInput {padding:3px 8px;}
.r_grid table td input[type=text] {height:25px; border:1px solid #c3c3c3;}
.r_grid table td input[type=number] {height:25px; border:1px solid #c3c3c3;}
.r_grid .ui-jqgrid .ui-pg-table tr {background:#f9fafb; height:35px}
.r_grid .ui-jqgrid .ui-pg-table tr:hover {background:#f9fafb;}
.r_grid .ui-jqgrid .ui-pg-table {border-top:1px solid #dedfe0;position: relative; padding-bottom:2px; width:auto; margin: 0;background:#f9fafb;}
.r_grid .ui-jqgrid .ui-pg-table td {font-weight:normal; vertical-align:middle; padding:1px;}
.r_grid .ui-jqgrid .ui-pg-table .ui-pg-table {border-top:none;}

