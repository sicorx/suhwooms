.r_search.oneLine table,
.r_inputTbl .r_search.oneLine table {width:auto; position:relative; display:inline-block;}
/* .r_search table th, .r_inputTbl .r_search.oneLine table th {text-align:left; background:none; border:none; padding:0; color:#333333;} */
.r_search table th, .r_inputTbl .r_search.oneLine table th {text-align:left; background:none; border:none; padding:0; font-family:"Malgun Gothic","NanumGothic", Dotum !important; color:#222222; font-size:12px; font-weight:normal !important;} /* 2017.02.01 수정 */
